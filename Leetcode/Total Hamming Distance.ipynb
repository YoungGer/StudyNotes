    "class Solution(object):\n",
    "    def totalHammingDistance(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        return sum(b.count('0') * b.count('1') for b in zip(*map('{:032b}'.format, nums)))"
   ]
