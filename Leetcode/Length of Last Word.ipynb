    "    def lengthOfLastWord(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        if not s.split():\n",
    "            return0\n",
    "        try:\n",
    "            return len(s.split()[-1])\n",
    "        except IndexError:\n",
    "            return0"
